<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean	id="scheduleController"
			class="com.mydiary.schedule.web.ScheduleController">
		<property name="scheduleService" ref="scheduleService"></property>	
	</bean>
	
	<bean	id="memberController"
			class="com.mydiary.member.web.MemberController">
		<property name="memberService" ref="memberService"></property>		
	</bean>
	
	<bean	id="memoController"
			class="com.mydiary.memo.web.MemoController">
		<property name="memoService" ref="memoService"></property>		
	</bean>

</beans>
